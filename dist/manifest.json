{
  "manifest_version": 3,
  "name": "TabTabTab",
  "version": "2.4.2",
  "description": "This chrome extension makes it easier to manage your open tabs.",

  "action": {
    "default_title": "TabTabTab",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "tabtabtab16.png",
      "32": "tabtabtab32.png",
      "48": "tabtabtab48.png",
      "128": "tabtabtab128.png"
    }
  },
  "permissions": [
    "alarms",
    "activeTab",
    "bookmarks",
    "tabGroups",
    "tabs",
    "storage"
  ],
  "background" : {
    "service_worker" : "js/background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "tabtabtab16.png",
    "32": "tabtabtab32.png",
    "48": "tabtabtab48.png",
    "128": "tabtabtab128.png"
  }
}
